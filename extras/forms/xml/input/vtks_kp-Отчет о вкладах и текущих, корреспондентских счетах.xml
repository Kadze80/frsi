<script>
<![CDATA[
function updateCalculatedFields() {
    var cn = [];
    cn[1] = "person";
    cn[2] = "jurper_bankres";
    cn[3] = "jurper_banknonres";
    cn[4] = "jurper_other";

    var sum;

    for (var col = 1; col <= 4; col++) {
        var colPrefix = "vtks_kp_array*" + cn[col] + ":num:";
        for (var i = 1; i <= 2; i++) {
            sum = 0;
            for (var j = 1; j <= 5; j++) {
                sum += getInputValue(colPrefix + "5." + j + "." + i);
            }
            setInputValue(colPrefix + "5." + i, sum);

            sum = 0;
            for (var j = 1; j <= 6; j++) {
                sum += getInputValue(colPrefix + j + "." + i);
            }
            setInputValue(colPrefix + "7." + i, sum);
        }
    }
}
]]>
</script>
