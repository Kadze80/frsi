<script>
<![CDATA[

function updateCalculatedFields() {

var argColumns = ["loan_long","loan_short","subs_debt","accpayable_long","accpayable_short","cb_repo","div_pay","other_sources"];

var i, gid;
var sum, totalSum;
var groupId;

for (i = 0; i <= 7; i++) {
	totalSum = 0;
	for (gid = 1; gid <= 10; gid++) {
		groupId = "fs_oi_array*" + argColumns[i] + ":num:" + gid;
		sum = sumInputValuesByGroup(groupId);
		setInputValue(groupId, sum);
		totalSum += sum;
	}
	setInputValue("fs_oi_array*" + argColumns[i] + ":num:Itogo", totalSum);
}

for (gid = 1; gid <= 10; gid++) {
    totalSum = 0;
    for (i = 0; i <= 7; i++) {
		groupId = "fs_oi_array*" + argColumns[i] + ":num:" + gid;
		totalSum += getInputValue(groupId);
	}
	var colGroupId = "fs_oi_array*vsego:num:" + gid;
	setInputValue(colGroupId, totalSum);
	updateDynamicRowsColumn(colGroupId, argColumns);
}
totalSum = 0;
for (i = 0; i <= 7; i++) {
	groupId = "fs_oi_array*" + argColumns[i] + ":num:Itogo";
	totalSum += getInputValue(groupId);
}
var colGroupId = "fs_oi_array*vsego:num:Itogo";
setInputValue(colGroupId, totalSum);
}
]]>
</script>
