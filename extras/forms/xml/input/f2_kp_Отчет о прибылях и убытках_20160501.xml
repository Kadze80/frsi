<script>
<![CDATA[
function updateCalculatedFields() {

var cn=[];
cn[3] = "pr_sum";
cn[4] = "pr_begthis_year_sum";
cn[5] = "simpr_prev_year_sum";
cn[6] = "simpr_begprev_year_sum";

for(var i=3; i <= 6; i++){

	var v1 = Cells(cn[i],"1");
	var v2 = Cells(cn[i],"2");
	var v3 = v1 - v2;
	Cells(cn[i],"3",v3);

	var v13 = SummLoop(cn[i],3,12);
	Cells(cn[i], "13", v13);


	var v20 = SummLoop(cn[i],14,19);
	Cells(cn[i], "20", v20);
	var v21 = v13 - v20;
	Cells(cn[i], "21", v21);

	var v22 = Cells(cn[i], "22");
	var v23 = v21 - v22;
	Cells(cn[i], "23", v23);

	var v24 = Cells(cn[i],"24");
	var v25 = v23 + v24;
	Cells(cn[i], "25", v25);
}
}
// Суммирование по диапазону
function SummLoop(cn, qkey1,qkey2){
	var summ=0;

	for (var i=qkey1; i<= qkey2; i++){
		var prefix = "f2_kp_array*"+cn+":code:"+i;
		var arg = Cells(cn, i);
		summ += arg;
	}

	return summ;
}
// Суммирование подпунктов
function SummGroup(cn, qkey, n){
var summ=0;
var prefix = "f2_kp_array*"+cn+":code:"+qkey;
for(var i=1; i<=n; i++){
	var arg = Cells(cn, qkey+"."+i);
	summ += arg;
}
setInputValue(prefix, summ);
return summ;
}
function Cells(cn, qrow){
var prefix = "f2_kp_array*"+cn+":code:"+qrow;
	if (arguments[2]||arguments[2]==0){
		setInputValue(prefix, arguments[2]);
	}
		return getInputValue(prefix);
	}
]]>
</script>
