<script>
<![CDATA[
function updateCalculatedFields() {
var cn=[];
cn[3] = "pr_sum";
cn[4] = "pr_begthis_year_sum";
cn[5] = "simpr_prev_year_sum";
cn[6] = "simpr_begprev_year_sum";


for(var i=3; i <= 6; i++){
	var v1_1 = Cells(cn[i], "1.1");
	var v1_2 = Cells(cn[i], "1.2");
	var v1_3 = Cells(cn[i], "1.3");
	var v1_4 = Cells(cn[i], "1.4");
	var v1_5 = Cells(cn[i], "1.5");
	var v1_6 = Cells(cn[i], "1.6");
	var v1_7 = Cells(cn[i], "1.7");
	var v1 = v1_1 + v1_2 + v1_3 + v1_4 + v1_5 + v1_6 + v1_7;
	Cells(cn[i], "1", v1);

	var v2 = Cells(cn[i], "2");

	var v3_1 = Cells(cn[i], "3.1");
	var v3_2 = Cells(cn[i], "3.2");
	var v3_3 = Cells(cn[i], "3.3");
	var v3_4 = Cells(cn[i], "3.4");
	var v3_5 = Cells(cn[i], "3.5");
	var v3_6 = Cells(cn[i], "3.6");
	var v3 = v3_1 + v3_2 + v3_3 + v3_4 + v3_5 + v3_6;
	Cells(cn[i], "3", v3);

	var v4 = Cells(cn[i], "4");
	var v5 = Cells(cn[i], "5");
	var v6 = Cells(cn[i], "6");
	var v7 = Cells(cn[i], "7");
	var v8 = Cells(cn[i], "8");
	var v9 = Cells(cn[i], "9");
	var v10 = Cells(cn[i], "10");

	var v11 = v1 + v2 + v3 + v4 + v5 + v6 + v7 + v8 + v9 + v10;
	Cells(cn[i], "11", v11);

	var v12_1 = Cells(cn[i], "12.1");
	var v12_2 = Cells(cn[i], "12.2");
	var v12_3 = Cells(cn[i], "12.3");
	var v12_4 = Cells(cn[i], "12.4");
	var v12_5 = Cells(cn[i], "12.5");
	var v12_6 = Cells(cn[i], "12.6");
	var v12 = v12_1 + v12_2 + v12_3 + v12_4 + v12_5 + v12_6;
	Cells(cn[i], "12", v12);

	var v13 = Cells(cn[i], "13");
	var v14 = Cells(cn[i], "14");
	var v15 = Cells(cn[i],"15");
	var v16 = Cells(cn[i],"16");
	var v17 = Cells(cn[i],"17");
	var v18 = Cells(cn[i],"18");
	var v19 = v12 + v13 + v14 + v15 + v16 + v17 + v18;
	Cells(cn[i], "19", v19);

	var v20 = v11-v19;
	Cells(cn[i], "20", v20);
	var v21 = Cells(cn[i], "21");
	var v22 = v20 - v21;
	Cells(cn[i], "22", v22);

	var v23 = Cells(cn[i],"23");
	var v24 = v22 + v23;
	Cells(cn[i], "24", v24);
}
}
// Суммирование подпунктов
function SummGroup(cn, qkey, n){
	var summ=0;
	var prefix = "f2_mfo_array*"+cn+":code:"+qkey;
	for(var i=1; i<=n; i++){
		var arg = Cells(cn, qkey+"."+i);
		summ += arg;
	}
	setInputValue(prefix,summ );
	return summ;
}
function Cells(cn, qrow){
var prefix = "f2_mfo_array*"+cn+":code:"+qrow;
	if (arguments[2]||arguments[2]==0){
		setInputValue(prefix,arguments[2]);
	}
	return getInputValue(prefix);
}
]]>
