<script>
<![CDATA[
function updateCalculatedFields() {
	var groupId, v1, v2, v3, sum;

	for (var i = 1; i < 7; i++) {
	    if (i != 4) {
			groupId = "t5_mfo_array*v" + i + ":n:";
			v1 = getInputValue(groupId + "1");
			v2 = getInputValue(groupId + "2.1") + getInputValue(groupId + "2.2") + getInputValue(groupId + "2.3") +
			     getInputValue(groupId + "2.4") + getInputValue(groupId + "2.5");
			setInputValue(groupId + "2", v2);
			v3 = getInputValue(groupId + "3");
			sum = v1 + v2 + v3;
			setInputValue(groupId + "Itogo", sum);
		}
	}
}
]]>
</script>
<script2>
