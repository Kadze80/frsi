<script>
<![CDATA[
function updateCalculatedFields() {
var res1=0;
var res2=0;
var res3=0;
var res4=0;
for (var i=1; i<=31; i++) {
	var A = getInputValue("k2_ot_array*highlyliquid_assets:num:"+i);
	var B = getInputValue("k2_ot_array*sum_oblig:num:"+i);
	var C = getInputValue("k2_ot_array*sum_netreq:num:"+i);
	var D = getInputValue("k2_ot_array*sum_maxdam:num:"+i);
	res1 += A;
	res2 += B;
	res3 += C;
	res4 += D;
}
setInputValue("k2_ot_array*highlyliquid_assets:num:Itogo", res1);
setInputValue("k2_ot_array*sum_oblig:num:Itogo", res2);
setInputValue("k2_ot_array*sum_netreq:num:Itogo", res3);
setInputValue("k2_ot_array*sum_maxdam:num:Itogo", res4);

}
]]>		
</script>
