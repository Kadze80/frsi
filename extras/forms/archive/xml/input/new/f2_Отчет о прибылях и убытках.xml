// Суммирование подпунктов
function SummGroup(cn, qkey, n){
var summ=0;
var prefix = "f2_array*"+cn+":code:"+qkey;
for(var i=1; i<=n; i++){
var arg = Cells(cn, qkey+"."+i);
summ += arg;
}			
setInputValue(prefix, summ);
return summ;
}
function Cells(cn, qrow){
var prefix = "f2_array*"+cn+":code:"+qrow;
if (arguments[2]){				
setInputValue(prefix, arguments[2]);
}
var x=getInputValue(prefix);
if (x==null) x=0;
return x;
}

